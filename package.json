{
  "name": "@tonejs/midi",
  "version": "1.1.0",
  "description": "Convert binary midi into JSON",
  "main": "build/Midi.js",
  "module": "src/Midi.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "webpack -p --mode=production",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "increment": "node scripts/increment_version.js",
    "lint": "eslint src/*.js",
    "watch": "webpack -w --mode=development",
    "test:watch": "nyc mocha --watch --timeout=10000 --require @babel/polyfill --require @babel/register ./test/Header.js",
    "test": "nyc mocha --timeout=10000 --require @babel/polyfill --require @babel/register ./test/*.js",
    "tsc": "tsc",
    "tsd": "jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/*.js && mv out/types.d.ts ./index.d.ts"
  },
  "files": [
    "./build/*",
    "./src/*",
    "LICENSE.md",
    "README.md",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tonejs/Midi.git"
  },
  "author": "Yotam Mann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tonejs/Midi/issues"
  },
  "homepage": "https://tonejs.github.com/Midi/",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.3",
    "array-flatten": "^2.1.2",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "codecov": "^3.2.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.3",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "node-fetch": "^2.3.0",
    "nyc": "^13.3.0",
    "semver": "^5.6.0",
    "tsd-jsdoc": "^2.1.3",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "midi-file": "^1.1.1"
  }
}
